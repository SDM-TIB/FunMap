<#TriplesMap8>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source <#DB_source>;
                        rr:tableName "PROJECT1","OUTPUT1";
                        rml:query "SELECT DISTINCT p.`Gene name`, o.`OUTPUT1` FROM PROJECT1 as p, OUTPUT1 as o WHERE p.`Gene name`=o.`Gene name`and p.`Mutation CDS`=o.`Mutation CDS`";];
    rr:subjectMap [
        rr:template "http://project-iasis.eu/Gene/{Gene name}";
    ];
    rr:predicateObjectMap [
        rr:predicate iasis:isRelatedTo;
        rr:objectMap [
        rml:reference "OUTPUT1"
        ]
    ].

