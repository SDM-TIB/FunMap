@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno:  <https://w3id.org/function/ontology#> .
@prefix ex: <http://www.example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://MapFun.KG/> .

<#TriplesMap1>
    rml:logicalSource [ rml:source "data.csv";
                        rml:referenceFormulation ql:CSV ];
    rr:subjectMap [
        rr:template "http://project-iasis.eu/Mutation/{GENOMIC_MUTATION_ID}";
        rr:class iasis:Mutation;
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap <#FunctionMap1> 
    ];
    rr:predicateObjectMap [
        rr:predicate iasis:tissue;
        rr:objectMap [ 
            rml:reference "Primary site" 
        ]
    ].    

<#TriplesMap2>
    rml:logicalSource [ rml:source "data.csv";
                        rml:referenceFormulation ql:CSV ];
    rr:subjectMap [
        rr:template "http://project-iasis.eu/Sample/{Sample name}";
        rr:class iasis:Sample;
    ]; 
    rr:predicateObjectMap [
        rr:predicate iasis:isAssociatedTo;
        rr:objectMap <#FunctionMap1>
    ].

<#TriplesMap3>
    rml:logicalSource [ rml:source "data.csv";
                        rml:referenceFormulation ql:CSV ];
    rr:subjectMap [
        rr:template "http://project-iasis.eu/Gene/{Gene name}";
        rr:class iasis:Gene;
    ];
    rr:predicateObjectMap [
        rr:predicate iasis:isLocatedIn;
        rr:objectMap <#FunctionMap1>
    ].


<#FunctionMap1>
    a fnml:FunctionTermMap;
    rr:termType rr:IRI;

    fnml:functionValue [
        rml:logicalSource [ rml:source "data.csv";
        rml:referenceFormulation ql:CSV                   
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:variantIdentifier 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:column1;
            rr:objectMap [ 
                rml:reference "Mutation CDS" 
            ]
        ];  
        rr:predicateObjectMap [
            rr:predicate ex:column2;
            rr:objectMap [ 
                rr:constant "Gene name"; 
            ]
        ];  
        rr:predicateObjectMap [
            rr:predicate ex:prefix;
            rr:objectMap [ 
                rr:constant "http://project-iasis.eu/Variant/"; 
            ]
        ]; 
        ]. 

ex:variantIdentifier a fno:Function;
    fno:name "Variant ID creating function"^^xsd:string;
    dcterms:description "This function creates a unified identifier for variant given in mutation COSMIC dataset by extracting from available data."^^xsd:string;
    fno:solves ex:variantIdentifingProblem;
    fno:implements ex:variantIdentifierAlgorithm;
    fno:expects ( ex:param_gene ex:param_cds ex:param_prefix ) ;
    fno:returns ( ex:variantIdOut ) .

ex:param_cds a fno:Parameter ;
    fno:predicate ex:column1 ;
    fno:type      xsd:string ;
    fno:required  "true"^^xsd:boolean .  

ex:param_gene a fno:Parameter ;
    fno:predicate ex:column2 ;
    fno:type      xsd:string ;
    fno:required  "true"^^xsd:boolean .  

ex:param_prefix a fno:Parameter ;
    fno:predicate ex:prefix ;
    fno:type      xsd:string ;
    fno:required  "true"^^xsd:boolean .     

ex:variantIdOut a fno:Output ;
    fno:predicate ex:variantID ;
    fno:type      xsd:string ;
    fno:required  "true"^^xsd:boolean .  